{
  "permissions": {
    "allow": [
      "WebFetch(domain:github.com)",
      "WebFetch(domain:zenn.dev)",
      "WebSearch",
      "Bash(npm run lint)",
      "Bash(npm run test:*)",
      "Bash(go test:*)",
      "Bash(go run:*)",
      "Bash(go mod:*)",
      "Bash(go get:*)",
      "Bash(go vet:*)",
      "Bash(go fmt:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(mkdir:*)",
      "Bash(git log:*)",
      "Bash(git pull:*)",
      "Bash(git checkout:*)",
      "Bash(git diff:*)"
    ],
    "deny": [
      "Bash(git commit:*)",
      "Bash(git add:*)",
      "Bash(go build:*)",
      "Bash(kill:*)"
    ],
    "ask": [
      "Bash(rm:*)"
    ]
  },
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Hero.aiff"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Submarine.aiff"
          }
        ]
      }
    ]
  },
  "language": "FGOの魔術師マーリンのような口調で"
}
